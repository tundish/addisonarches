<!--(macro style)-->
<style>
h1, h2, h2 {
font-family: "Trebuchet MS", Helvetica, sans-serif;
color: #666666;
}

.local-nav {
width: 22em;
}

ol.local-items, nav ul {
list-style-type: none;
}

li {
border: 2px solid white;
background-color: #ccc5a7;
padding: 0.1em;
margin: 0em 0.3em 0em 0.6em;
}

li.tick{
background-color: #afa7cc;
}

li.drama{
background-color: #afa7cc;
}

li.item {
background-color: #d1f274;
}

li.patter{
background-color: #fff9e0;
}

li.patter blockquote header::after{
content: ": ";
}

li.patter blockquote p::after{
content: close-quote;
}

li.patter blockquote p::before{
content: open-quote;
}

nav ul li {
background-color: white;
border: 0px;
margin: 0.2em 1.0em 0.2em 0.6em;
padding: 0.3em;
}

ol.local-items dl dt {
display: none;
}

ol.local-items dl dd {
display:inline-block;
margin:0.2em;
}

ol.local-items form {
display:inline-block;
margin:0.2em;
}

</style>
<!--(end)-->
<!--(if exists("unittest"))-->
@!style()!@
<!--(end)-->
