<!--(set_escape)-->
    html
<!--(end)-->
<!--(macro style)-->
<style>
h1, h2, h2 {
font-family: "Trebuchet MS", Helvetica, sans-serif;
color: #666666;
}

h3 {
text-transform: capitalize;
}

.hide {
display: none;
}

.local-nav {
width: 22em;
}

ol, nav ul {
list-style-type: none;
}

li {
font-family: "Trebuchet MS", Helvetica, sans-serif;
color: #444444;
border: 2px solid white;
background-color: #ccc5a7;
padding: 0.1em;
margin: 0em 0.3em 0em 0.3em;
}

li.tick{
background-color: #afa7cc;
}

li.drama{
background-color: #afa7cc;
}

li.item {
background-color: #d1f274;
}

li.patter{
background-color: #fff9e0;
}

li.patter blockquote header::after{
content: ": ";
}

li.patter blockquote p::after{
content: close-quote;
}

li.patter blockquote p::before{
content: open-quote;
}

nav ul li {
background-color: white;
border: 0px;
margin: 0.2em 1.0em 0.2em 0.6em;
padding: 0.3em;
}

ol dl {
display:inline-block;
}

ol dl dt {
clear: left;
float: left;
padding-left: 2.5em;
}

ol.local-summary dl dt {
text-transform: capitalize;
}

ol.local-summary dl dt::after {
content: ": ";
margin-left: 0.5em;
}

ol dl dd {
overflow: hidden;
text-overflow: ellipsis;
white-space: nowrap;
}

ol.local-items dl dt{
clear: left;
float: right;
display: inline;
}

ol.local-items dl dd{
clear: left;
float: right;
display: inline;
}

</style>
<!--(end)-->
<!--(if exists("unittest"))-->
@!style()!@
<!--(end)-->
